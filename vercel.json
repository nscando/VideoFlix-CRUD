{
  "name": "videoflix",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@videoflix_db_user",
    "DB_PASSWORD": "@videoflix_db_password",
    "DB_HOST": "@videoflix_db_host",
    "DB_NAME": "@videoflix_db_name"
  }
}