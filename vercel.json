{
  "name": "videoflix",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@videoflix-db-user",
    "DB_PASSWORD": "@videoflix-db-password",
    "DB_HOST": "@videoflix-db-host",
    "DB_NAME": "@videoflix-db-name"
  }
}